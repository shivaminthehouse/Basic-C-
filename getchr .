#include<stdio.h>
#include<conio.h>
void main()
{
  char a,b;
  clrscr();
   printf("\nEnter one character:");
   scanf("%c",&a);
   printf("Input character is ==> %c",a);
   fflush(stdin);//to clear input buffer
   printf("\n\n\nEnter second character:");
   b=getchar();

   printf("\nASCII value of character(%c)is=> %d",b,b);
getch();
}
